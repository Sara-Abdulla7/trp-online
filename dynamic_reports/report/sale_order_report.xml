<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

    <template id="sale_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="dynamic_reports.kefah_layout">
                    <div class="page customfont">
                        <table class="table table-striped table-condensed"
                               style="text-align: center; font-family:'Calibri';width:100%; height:5px;border:1px solid white;font-size:75%">
                            <tr style="height:5px;">
                                <td width="50%" align="left" style="font-family:'Calibri';background:#e6e6e6;padding:0;font-size:15px;">
                                    <strong>
                                        <span t-if="o.state in ['draft', 'sent']" style="height: 50px;">Sale Quotation:</span>
                                        <span t-if="o.state in ['done', 'sale', 'cancel']">Sale Order:</span>
                                        <span t-field="o.name"/>
                                    </strong>
                                </td>
                                <td width="50%" align="right" style="font-family:'Cairo';background:#e6e6e6;padding:0;font-size:15px;">
                                    <strong>
                                        <span t-field="o.name"/>
                                        <span t-if="o.state in ['draft', 'sent']">: عرض السعر</span>
                                        <span t-if="o.state in ['done', 'sale', 'cancel']">: أمر بيع</span>
                                    </strong>
                                </td>
                            </tr>
                            <tr class="order-line">
                                <td width="50%" align="left" style="background:#e6e6e6;padding:0;font-size:15px;">
                                    <strong>
                                        <span>Sale Date :</span>
                                        <span t-field="o.date_order"/>
                                    </strong>
                                </td>
                                <td width="50%" align="right" style="background:#e6e6e6;padding:0;font-size:15px;">
                                    <strong>
                                        <span t-field="o.date_order"/>
                                        <span>: تاريخ الفاتورة</span>
                                    </strong>
                                </td>
                            </tr>
                            <tr class="order-line">
                                <td width="50%" align="left" style="background:#e6e6e6;padding:0;font-size:15px;">
                                    <strong>
                                        <span>Sales Person :</span>
                                        <span t-field="o.user_id"/>
                                    </strong>
                                </td>
                                <td width="50%" align="right" style="background:#e6e6e6;padding:0;font-size:15px;">
                                    <strong>
                                        <span t-field="o.user_id"/>
                                        <span>: المسئول</span>
                                    </strong>
                                </td>
                            </tr>
                        </table>
<!--                        <br/>-->
                        <table class="table table-borderless" style="width:100%; background:#80ccff;">
                            <tr class="order-line">
                                <td  align="left" style="padding:0;font-size:15px;">
                                    <strong>
                                        <span>Company Information</span>
                                    </strong>
                                </td>
                                <td align="right" style="padding:0;font-size:15px;">
                                    <strong>
                                        <span>معلومات الشركة</span>
                                    </strong>
                                </td>
                            </tr>
                        </table>
<!--                        <br/>-->
                        <table class="table table-striped table-condensed" style="width:100%;border:1px solid white;">
                            <tr class="order-line" style="font-family:'Calibri'; background:#e6f7ff;">
                                <td width="50%" align="left" style="padding:0;font-size:15px;">
                                    <strong>
                                        <span t-field="o.company_id.name"/>
                                    </strong>
                                </td>
                                <td width="50%" align="right" style="padding:0;font-size:15px;">
                                    <strong>
